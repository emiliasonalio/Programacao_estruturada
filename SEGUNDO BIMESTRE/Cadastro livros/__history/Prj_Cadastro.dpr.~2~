program Prj_Cadastro;

uses
  Vcl.Forms,
  Frm_Cadastro in 'Frm_Cadastro.pas' {Form1},
  uDM_cadastro_livros in 'uDM_cadastro_livros.pas' {uDM_Cadastro: TDataModule};

